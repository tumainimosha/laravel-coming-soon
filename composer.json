{
    "name": "tumainimosha/laravel-coming-soon",
    "type": "library",
    "description": "Simple Comming Soon page",
    "license": "MIT",
    "authors": [
        {
            "name": "Tumaini Mosha",
            "email": "princeton.mosha@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "orchestra/testbench": "~3.7.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "snw\\ComingSoon\\ComingSoonServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "snw\\ComingSoon\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
